{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "excludes": [
        "api/models/**",
        "api/utils/**",
        "api/middleware/**",
        "api/scripts/**"
    ]
}